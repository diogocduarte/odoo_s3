<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_form_inherit_print" model="ir.ui.view">
            <field name="name">res.config.settings.inherit</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@id='module_auth_oauth']" position="before">
                    <div class="col-12 col-lg-6 o_setting_box" id="module_odoo_s3">
                        <div class="o_setting_right_pane">
                            <span class="o_form_label">AWS S3 Storage</span>
                            <div class="content-group">
                                <div class="mt16 row">
                                    <label for="s3_profile" class="col-lg-3 o_light_label"/>
                                    <field name="s3_profile"/>
                                </div>
                                <div class="mt16 row">
                                    <label for="s3_bucket" class="col-lg-3 o_light_label"/>
                                    <field name="s3_bucket"/>
                                </div>
                                <div class="mt16 row">
                                    <label for="s3_load" class="col-lg-3 o_light_label"/>
                                    <field name="s3_load"/>
                                </div>
                                <div class="mt16 row">
                                    <button type="object" name="test_move_filestore_to_s3" string="Test S3 Settings"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
